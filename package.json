{
  "name": "blockchain-docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node build/bootstrap",
    "start:watch": "nodemon",
    "start:typescript": "ts-node --pretty --project 'tsconfig.json' src/bootstrap.ts",
    "test": "jest --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:staged": "lint-staged",
    "format:staged": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged && npm run format:staged"
    }
  },
  "lint-staged": {
    "!(*test).ts": [
      "tslint -c 'tslint.json' -p 'tsconfig.json' -t codeFrame  --fix "
    ]
  },
  "keywords": [],
  "author": "gustavolizze",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^13.13.5",
    "@types/reflect-metadata": "^0.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "reflect-metadata": "^0.1.13"
  }
}
